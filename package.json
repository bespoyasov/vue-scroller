{
  "name": "vue-prokrutchik",
  "version": "2.0.0",
  "description": "Vue wrapper for Prokrutchik",
  "keywords": [
    "scroller",
    "scroll",
    "drag",
    "drag-n-drop",
    "vue"
  ],
  "author": "Alex Bespoyasov <bespoyasov@me.com>",
  "repository": "bespoyasov/vue-scroller",
  "type": "module",
  "module": "vue-scroller.js",
  "browser": "vue-scroller.browser.js",
  "style": "vue-scroller.css",
  "exports": {
    ".": "./vue-scroller.js",
    "./package.json": "./package.json",
    "./styles.css": "./vue-scroller.css",
    "./browser.js": "./vue-scroller.browser.js"
  },
  "files": [
    "vue-scroller.js",
    "vue-scroller.browser.js",
    "vue-scroller.d.ts",
    "vue-scroller.css"
  ],
  "scripts": {
    "build": "npm run build:browser & npm run build:module",
    "build:module": "rollup --format es --file vue-scroller.js",
    "build:browser": "rollup --format iife --file vue-scroller.browser.js"
  },
  "peerDependencies": {
    "prokrutchik": "^3.0.1",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "rollup": "^2.75.5",
    "rollup-plugin-vue": "^6.0.0",
    "vite": "^2.9.9"
  }
}
